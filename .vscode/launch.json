{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "rust_code_examples",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/rs/target/debug/rust_code_examples.exe",
      "args": [],
      "cwd": "${workspaceFolder}/rs",
      "stopAtEntry": false,
      "environment": [],
      "console": "integratedTerminal",
      "preLaunchTask": "cargo build",
      "miDebuggerPath": "C:/Program Files (x86)/Windows Kits/10/Debuggers/x64/cdb.exe"
    }
  ]
}
